<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>

<VBox xmlns:fx="http://javafx.com/fxml/1" styleClass="vbox" fx:controller="MyController" fx:id="root" alignment="CENTER" spacing="10">
    <Button fx:id="playButton" text="PLAY" onAction="#menuPlayButtonClick"/>
    <Button fx:id="quitButton" text="QUIT" onAction="#menuQuitButtonClick"/>
</VBox>
